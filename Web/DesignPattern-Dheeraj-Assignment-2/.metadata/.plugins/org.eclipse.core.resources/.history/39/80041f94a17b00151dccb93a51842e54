<%@page import="com.model.Car"%>
<%@page import="com.db.helper.CarDBHelper"%>
<%@page import="java.sql.Connection" %>>
<html>
	<head>
		<title>get and set properties Example</title>
	</head>
	<body>
	<jsp:useBean id="car" class="com.model.Car" scope="page">
		<jsp:setProperty name="car" property="*"/>
	</jsp:useBean>
	<%
		System.out.println("Make year is "+car.getMakeYear());
		//	System.out.println(car.getId());
		System.out.println(car.getCreatedBy());
		System.out.println(car.getEngineInCC());
		System.out.println(car.getFuelCapacity());
		System.out.println(car.getMilage());
		System.out.println(car.getModel());
		System.out.println(car.getPrice());
		System.out.println(car.getRoadTax());
		System.out.println("time is "+ car.getCreatedTime());
		System.exit(1);
			
		CarDBHelper carDBHelper = new CarDBHelper(); 
		Boolean isInserted = carDBHelper.create(car);
		if(isInserted)
			out.print("successfully inserted");
		else
			out.print("not inserted");
	%>
	</body>
</html>
