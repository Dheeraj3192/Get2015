<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TemperatureConverterService WSDL service -->

  <service name="TemperatureConverter" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservice.com"/>
      <parameter name="wsdlServiceElement" value="TemperatureConverterService"/>
      <parameter name="schemaQualified" value="http://webservice.com"/>
      <parameter name="wsdlServicePort" value="TemperatureConverter"/>
      <parameter name="className" value="com.webservice.TemperatureConverterSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TemperatureConverter"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="convert" qname="operNS:convert" xmlns:operNS="http://webservice.com" returnQName="retNS:convertReturn" xmlns:retNS="http://webservice.com" returnType="rtns:float" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:converterType" xmlns:pns="http://webservice.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:input" xmlns:pns="http://webservice.com" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="convert"/>

  </service>
</deployment>
